{
  "name": "guild-mng-bot-v2",
  "version": "2.0.0",
  "description": "Discord Guild Management Bot with Web UI",
  "main": "dist/bot/main.js",
  "type": "module",
  "scripts": {
    "dev:bot": "tsx watch src/bot/main.ts",
    "dev:web": "tsx watch src/web/server.ts",
    "dev": "concurrently \"pnpm dev:bot\" \"pnpm dev:web\"",
    "build": "tsc --build",
    "tsc-watch": "tsc --build --watch",
    "start:bot": "node dist/bot/main.js",
    "start:web": "node dist/web/server.js",
    "start": "concurrently \"pnpm start:bot\" \"pnpm start:web\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:push": "prisma db push"
  },
  "keywords": [
    "discord",
    "bot",
    "guild-management"
  ],
  "author": "",
  "license": "Apache-2.0",
  "packageManager": "pnpm@10.30.1",
  "engines": {
    "node": ">=24"
  },
  "dependencies": {
    "@fastify/cors": "^11.2.0",
    "@fastify/static": "^9.0.0",
    "@libsql/client": "^0.17.0",
    "@prisma/adapter-libsql": "^7.4.1",
    "@prisma/client": "^7.4.1",
    "discord.js": "^14.25.1",
    "dotenv": "^17.3.1",
    "fastify": "^5.7.4",
    "i18next": "^25.8.13",
    "node-cron": "^4.2.1",
    "prisma": "^7.4.1",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.2",
    "@commitlint/config-conventional": "^20.4.2",
    "@types/node": "^25.3.0",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "@vitest/coverage-v8": "^4.0.18",
    "concurrently": "^9.2.1",
    "eslint": "^10.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "prettier": "^3.8.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
